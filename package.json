{
  "name": "boehringer-ingelheim",
  "private": true,
  "version": "1.0.0",
  "author": "junaidparamberi@gmail.com",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.5.0",
    "brew": "^0.0.8",
    "d3": "^7.9.0",
    "dom": "^0.0.3",
    "framer-motion": "^11.5.6",
    "gsap": "^3.12.5",
    "jquery": "^3.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-globe.gl": "^2.27.2",
    "react-pulse": "^1.0.2",
    "react-router-dom": "^6.26.2",
    "react-spinners": "^0.14.1",
    "react-svg": "^16.1.34",
    "react-tsparticles": "^2.12.2",
    "router": "^2.0.0",
    "styled-components": "^6.1.13",
    "tsparticles": "^3.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-imagemin": "^0.4.6"
  },
  "main": "dist-electron/main.js",
  "build": {
    "appId": "boehringer.id",
    "productName": "Boehringer Ingelheim",
    "files": [
      "dist/",
      "dist-electron/",
      "node_modules/",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": [
        "portable",
        "zip",
        "nsis"
      ],
      "icon": "src/assets/icons/Icon_Accent Green.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "zip"
      ],
      "icon": "src/assets/icons/Icon_Accent-Green.png"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/assets/icons/Icon_Accent Green.icns",
      "sign": false
    }
  }
}
